{
    "files.exclude": {
        "**/*.{js}": { "when": "$(basename).ts" },
        "spec/**/*.{js,d.ts}": { "when": "$(basename).ts" },
        "**/{FeatureServiceLayer,index,QueryError,utils}.{d.ts}": { "when": "$(basename).ts" },
        "**/.git": true,
        "**/.svn": true,
        "**/.hg": true,
        "**/CVS": true,
        "**/.DS_Store": true
    },
    "typescript.tsdk": "node_modules\\typescript\\lib"
}